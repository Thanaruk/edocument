<SCRIPT type=3Dtext/javascript> =0A=
var xmlhttp =3D null;=0A=
var path =3D "";=0A=
var outputFunction =3D "";=0A=
var proxypath =3D "http://www.learningapi.com/scripts/yservproxy.php";=0A=
var DEBUG=3Dfalse;=0A=
function callContextService(query, context, outputFunc){=0A=
if ( DEBUG ) { alert('entering callContextService'); }=0A=
//replace whitespace in query and context string with '+'=0A=
context =3D context.replace(/ /g, '+');=0A=
query =3D query.replace(/ /g, '+');=0A=
// The web services request minus the domain name=0A=
path =3D =
"ContentAnalysisService/V1/termExtraction?appid=3DlearningAPI&output=3Djs=
on&query=3D" + query + "&context=3D" + context;=0A=
path =3D "yws_path=3D" + encodeURIComponent(path);=0A=
// Set the global outputFunction to the name of the processing function =
we want to use for the output.=0A=
// Which one we want will depend on the JSON structure we're expecting =
back from this particular type of query.=0A=
outputFunction =3D outputFunc;=0A=
//make the call to the server=0A=
callService(path);=0A=
}=0A=
function callService(path){=0A=
//create the XMLHttpRequest=0A=
if (window.XMLHttpRequest) {=0A=
xmlhttp =3D new XMLHttpRequest();=0A=
//xmlhttp.overrideMimeType('text/xml');=0A=
} else if (window.ActiveXObject) {=0A=
xmlhttp =3D new ActiveXObject("Microsoft.XMLHTTP");=0A=
} else {=0A=
alert('Perhaps your browser does not support xmlhttprequests?');=0A=
}=0A=
// Create the full path to the PHP proxy=0A=
var url =3D proxypath;=0A=
if ( DEBUG ) { alert('in callService: '+url); }=0A=
//assign the callback function to be run upon a response=0A=
xmlhttp.onreadystatechange =3D processResponse;=0A=
//make the call=0A=
xmlhttp.open('POST', url, true);=0A=
xmlhttp.setRequestHeader("Content-type", =
"application/x-www-form-urlencoded");=0A=
xmlhttp.setRequestHeader("Content-length", path.length);=0A=
xmlhttp.setRequestHeader("Connection", "close");=0A=
xmlhttp.send(path);=0A=
}=0A=
function processResponse() {=0A=
if (xmlhttp.readyState =3D=3D 4 ) {=0A=
if (xmlhttp.status =3D=3D 200) {=0A=
if ( DEBUG ) { alert('in processResponse: '+xmlhttp.responseText); }=0A=
//turn the JSON string returned into a JSON object=0A=
obj =3D (eval ( "(" + xmlhttp.responseText +")"));=0A=
// Then call the desired processing and display function set in the =
initial call.=0A=
// Here, the global String variable 'outputFunction' is the name of the =
function to be called=0A=
// that formats the data for display. So if 'outputFunction' is =
'processJSONKeywordString',=0A=
// then this next line calls processJSONKeywordString(obj)=0A=
eval(outputFunction)(obj);=0A=
} else {=0A=
alert("there was a problem with the request" + xmlhttp.status);=0A=
}=0A=
}=0A=
}=0A=
//turns the results string into HTML and inserts it into resultsdiv=0A=
// expects the form: {"ResultSet":{"Result":["excel =
spreadsheet","wikis"]}}=0A=
// turns it into a HTML bulleted list of keywords=0A=
function processJSONKeywordString(obj){=0A=
if (DEBUG) { alert(JSON.stringify(obj)); }=0A=
var output =3D JSON.stringify(obj.ResultSet.Result);=0A=
var delimregexp =3D (/\",\"/g);=0A=
var beginregexp =3D ("[\"");=0A=
var endregexp =3D ("\"]");=0A=
output =3D output.replace(delimregexp, "<li>");=0A=
output=3D output.replace(beginregexp, "<ul><li>");=0A=
output=3D output.replace(endregexp, "</ul>");=0A=
output +=3D "\n<!-- HERE IS THE RAW JSON =
RESPONSE\n\n"+JSON.stringify(obj)+"\n\n-->";=0A=
var resultsdiv =3D document.getElementById("results");=0A=
if ( DEBUG ) { alert('in processJSONKeywordString setting =
'+resultsdiv+'.innerHTML to: '+output); }=0A=
resultsdiv.innerHTML =3D output;=0A=
}=0A=
</SCRIPT>
